<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd 
	">

    <util:list id="messages">
        <value>/WEB-INF/messages</value>
        <value>/WEB-INF/tiles/messages</value>
        
        <value>/views/home/messages</value>
        <value>/views/security/messages</value>		
        <value>/views/welcome/messages</value>	
        <value>/views/administrator/messages</value>
        <value>/views/researcher/messages</value>	
        <value>/views/socialnetwork/messages</value>	
        <value>/views/misc/messages</value>
        <value>/views/confirmation/messages</value>
        <value>/views/app/messages</value>	
    </util:list>

</beans>